class AIEntity {
    function fact(x: int) -> int {
        match x {
            case 0 { return 1 }
            case 1 { return 1 }
            else {
                return x*fact(x-1)
            }        
        }        
    }
    
    function fact(x: int) -> int = match x {
        case 0 => 0
        case 1 => 1
        else => x*fact(x-1)    
    }

    function abs(x: int) -> int = match x {
        case x when x < 0 => -x
        else => x
    }    
}

